#!/usr/bin/env fish
# fish_add_path (status dirname)
dot-show-art
argparse h/help c/config i/init s/sync t/tui -- $argv

[ (count $argv) -ge 1 ] && dot-help && exit 1

set --query _flag_help && dot-help && exit 0
set --query _flag_config && dot-config && exit 0
set --query _flag_init && dot-init && exit 0
set --query _flag_sync && dot-sync && exit 0
set --query _flag_tui && dot-tui && exit 0

[ $status -ne 0 ] && clear && exit 67
dot-sync
