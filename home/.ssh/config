Include ~/.ssh/config.local

Host *
  SendEnv COLORTERM
  SetEnv TERM=xterm-256color

  Match exec "test $(uname -s) = Darwin"
    IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"

  Match exec "test $(uname -s) = Linux"
    IdentityAgent ~/.1password/agent.sock

  Match host github.com exec "pwd | grep -i papa"
    HostName github.com
    User git
    IdentityFile ~/.ssh/papa
    IdentitiesOnly yes

  Match host roly.sh,roly.ssh,ssh.roly.sh exec "command -v cloudflared"
    ProxyCommand cloudflared access ssh --hostname ssh.roly.sh
